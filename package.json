{
  "name": "organism-react-scroll-nav",
  "version": "0.3.4",
  "description": "A React scroll spy library with flux",
  "repository": "react-atomic/organism-react-scroll-nav",
  "main": "./build/cjs/src/index.js",
  "module": "./build/es/src/index.js",
  "keywords": [
    "react",
    "react-component",
    "organism",
    "atomic-ui"
  ],
  "author": "Hill <hill@kimo.com>",
  "license": "MIT",
  "dependencies": {
    "get-object-value": "*",
    "get-scroll-info": "*",
    "getoffset": "*",
    "immutable": "^3.8.0",
    "react-atomic-molecule": "*",
    "reshow-flux": "*",
    "smooth-scroll-to": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.5",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-pure-class-to-function": "*",
    "babel-preset-env": "*",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "webpack": "^1.13.0"
  },
  "scripts": {
    "build:cjs": "BABEL_ENV=build babel src -d build/cjs/src && BABEL_ENV=build babel ui -d build/cjs/ui",
    "build:es": "BABEL_ENV=es babel src -d build/es/src && BABEL_ENV=es babel ui -d build/es/ui",
    "build": "rm -rf build && npm run build:cjs && npm run build:es",
    "prepublish": "npm run build"
  },
  "files": [
    "build",
    "package.json",
    "README.md"
  ]
}
